<Window x:Class="Lab_5_Zadanie_Domowe_3.PobierzZadanieWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab_5_Zadanie_Domowe_3"
        mc:Ignorable="d"
        Title="PobierzZadanieWindow" Height="200" Width="350">
    <StackPanel Margin="10">
        <TextBlock x:Name="OpisTextBlock" TextWrapping="Wrap" Margin="0 0 0 10"/>
        <TextBlock x:Name="DataTextBlock" Margin="0 0 0 10"/>
        <Button Content="OK" Click="Ok_Click"/>
    </StackPanel>
</Window>
